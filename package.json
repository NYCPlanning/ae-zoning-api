{
  "name": "ae-zoning-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run redoc:build && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"db/**/*.ts\"",
    "dev": "npm run redoc:build && nest start --watch",
    "start": "node dist/main",
    "start:debug": "npm run redoc:build && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pg:configure": "ts-node ./db/query.ts configure",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:generate": "drizzle-kit generate",
    "redoc:build": "redocly build-docs openapi/openapi.yaml --output openapi/index.html",
    "redoc:dev": "redocly preview-docs openapi/openapi.yaml",
    "redoc:lint": "redocly lint openapi/openapi.yaml",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "kubb:generate": "kubb generate"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/serve-static": "^5.0.3",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.9.0",
    "drizzle-orm": "^0.36.1",
    "immer": "^10.1.1",
    "joi": "^17.13.3",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.14.0",
    "@faker-js/faker": "^8.3.1",
    "@kubb/cli": "^3.10.5",
    "@kubb/core": "^2.23.3",
    "@kubb/swagger": "^2.28.4",
    "@kubb/swagger-client": "^2.28.4",
    "@kubb/swagger-ts": "^2.28.4",
    "@kubb/swagger-zod": "^2.28.4",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@redocly/cli": "^1.34.2",
    "@types/express": "^4.17.17",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.11.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.6",
    "faker-js": "^1.0.0",
    "geojson": "npm:empty-npm-package@^1.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "20.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "setupFiles": [
      "dotenv/config"
    ],
    "modulePaths": [
      "<rootDir>/../"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'npm run typecheck'",
      "npm run lint"
    ]
  }
}
